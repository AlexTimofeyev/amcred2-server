{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "body": {
      "type": "blocks"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "block": {
      "type": "dynamiczone",
      "components": [
        "shared.seo"
      ]
    },
    "description": {
      "type": "text"
    },
    "kkk": {
      "type": "customField",
      "options": {
        "preset": "defaultMarkdown"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
