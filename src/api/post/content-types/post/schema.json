{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "title_ru": {
      "type": "string"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "inversedBy": "posts"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "body": {
      "type": "text"
    },
    "post_status": {
      "type": "enumeration",
      "enum": [
        "published",
        "pending",
        "blocked"
      ],
      "default": "pending"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "Condition": {
      "type": "dynamiczone",
      "components": [
        "shared.credit-info"
      ]
    }
  }
}
