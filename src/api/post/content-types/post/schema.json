{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "inversedBy": "posts"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "BORROW_MONEY",
        "LAND_MONEY"
      ]
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "posts"
    },
    "contact": {
      "type": "dynamiczone",
      "components": [
        "shared.contact-info"
      ]
    },
    "conditions": {
      "type": "dynamiczone",
      "components": [
        "post.credits"
      ]
    },
    "body": {
      "type": "richtext"
    },
    "companyName": {
      "type": "string"
    },
    "seoIndex": {
      "type": "boolean",
      "default": false
    }
  }
}
